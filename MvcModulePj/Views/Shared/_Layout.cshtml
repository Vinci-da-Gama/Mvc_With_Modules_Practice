@{ 
    ViewBag.Title = "Module_Practice";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="~/Content/imgs/favicon.ico" type="image/x-icon" />
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/jslibs")
    @Styles.Render("~/Content/csslibs")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @*
        Previously, here is navbar.
    *@
    <div class="ui fluid container">
        @RenderBody()
        <h4 class="ui horizontal divider header">
            <i class="tag icon"></i>
            Split page and Footer.
        </h4>
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/bootstrap")*@
    @Styles.Render("~/Content/custCss")
    @RenderSection("scripts", required: false)
</body>
</html>


